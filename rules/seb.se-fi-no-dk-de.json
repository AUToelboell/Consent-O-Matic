{
  "$schema": "https://raw.githubusercontent.com/cavi-au/Consent-O-Matic/master/rules.schema.json",
  "seb.se-fi-no-dk-de": {
      "detectors": [
          {
              "presentMatcher": [
                  {
                      "type": "css",
                      "target": {
                          "selector": ".pw-cookie-consent-v2"
                      }
                  }
              ],
              "showingMatcher": [
                  {
                      "type": "css",
                      "target": {
                          "selector": ".pw-cookie-consent-v2__main"
                      }
                  }
              ]
          }
      ],
      "methods": [
          {
              "name": "HIDE_CMP"
          },
          {
              "name": "OPEN_OPTIONS"
          },
          {
              "action": {
                  "type": "list",
                  "actions": [
                      {
                          "type": "consent",
                          "consents": [
                              {
                                  "matcher": {
                                      "type": "css",
                                      "target": {
                                          "selector": "label[for=cc-cat-1] > span"
                                      }
                                  },
                                  "trueAction": {
                                      "type": "click",
                                      "target": {
                                          "selector": "label[for=cc-cat-1] > span"
                                      }
                                  },
                                  "type": "B"
                              }
                          ]
                      },
                      {
                          "type": "consent",
                          "consents": [
                              {
                                  "matcher": {
                                      "type": "css",
                                      "target": {
                                          "selector": "label[for=cc-cat-32] > span"
                                      }
                                  },
                                  "trueAction": {
                                      "type": "click",
                                      "target": {
                                          "selector": "label[for=cc-cat-32] > span"
                                      }
                                  },
                                  "type": "E"
                              }
                          ]
                      }
                  ]
              },
              "name": "DO_CONSENT"
          },
          {
              "action": {
                  "type": "click",
                  "target": {
                      "selector": ".pw-cookie-consent-v2__actions > button:nth-of-type(1)"
                  }
              },
              "name": "SAVE_CONSENT"
          },
          {
              "name": "UTILITY"
          }
      ]
  }
}
