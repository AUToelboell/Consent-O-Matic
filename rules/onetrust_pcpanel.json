{
    "onetrust_pcpanel": {
        "detectors": [
            {
                "presentMatcher": {
                    "type": "css",
                    "target": {
                        "childFilter": {
                            "target": {
                                "selector": ".otPcPanel, .otPcCenter"
                            }
                        },
                        "selector": "#onetrust-consent-sdk"
                    }
                },
                "showingMatcher": {
                    "type": "css",
                    "target": {
                        "selector": "#onetrust-banner-sdk",
                        "displayFilter": true
                    }
                }
            }
        ],
        "methods": [
            {
                "action": {
                    "type": "click",
                    "target": {
                        "selector": "#onetrust-pc-btn-handler, .ot-sdk-show-settings"
                    }
                },
                "name": "OPEN_OPTIONS"
            },
            {
                "action": {
                    "type": "foreach",
                    "target": {
                        "selector": ".ot-cat-item"
                    },
                    "action": {
                        "type": "list",
                        "actions": [
                            {
                                "type": "ifcss",
                                "target": {
                                    "selector": ".ot-cat-header",
                                    "textFilter": "Analytics Cookies"
                                },
                                "trueAction": {
                                    "type": "consent",
                                    "consents": [
                                        {
                                            "matcher": {
                                                "type": "checkbox",
                                                "target": {
                                                    "selector": "input"
                                                }
                                            },
                                            "toggleAction": {
                                                "type": "click",
                                                "target": {
                                                    "selector": "input"
                                                }
                                            },
                                            "type": "B"
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "ifcss",
                                "target": {
                                    "selector": ".ot-cat-header",
                                    "textFilter": "Functional Cookies"
                                },
                                "trueAction": {
                                    "type": "consent",
                                    "consents": [
                                        {
                                            "matcher": {
                                                "type": "checkbox",
                                                "target": {
                                                    "selector": "input"
                                                }
                                            },
                                            "toggleAction": {
                                                "type": "click",
                                                "target": {
                                                    "selector": "input"
                                                }
                                            },
                                            "type": "A"
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "ifcss",
                                "target": {
                                    "selector": ".ot-cat-header",
                                    "textFilter": "Targeting Cookies"
                                },
                                "trueAction": {
                                    "type": "consent",
                                    "consents": [
                                        {
                                            "matcher": {
                                                "type": "checkbox",
                                                "target": {
                                                    "selector": "input"
                                                }
                                            },
                                            "toggleAction": {
                                                "type": "click",
                                                "target": {
                                                    "selector": "input"
                                                }
                                            },
                                            "type": "F"
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                },
                "name": "DO_CONSENT"
            },
            {
                "action": {
                    "type": "click",
                    "target": {
                        "selector": ".save-preference-btn-handler"
                    }
                },
                "name": "SAVE_CONSENT"
            },
            {
                "action": {
                    "type": "list",
                    "actions": [
                        {
                            "type": "hide",
                            "target": {
                                "selector": "#onetrust-consent-sdk"
                            }
                        }
                    ]
                },
                "name": "HIDE_CMP"
            }
        ]
    }
}