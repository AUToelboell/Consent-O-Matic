{
    "trustarcframe": {
        "detectors": [
            {
                "presentMatcher": {
                    "type": "css",
                    "target": {
                        "selector": "title",
                        "textFilter": "TrustArc Preference Manager"
                    }
                },
                "showingMatcher": {
                    "type": "css",
                    "target": {
                        "selector": ".gdpr .switch span.on, .pdynamicbutton a"
                    }
                }
            }
        ],
        "methods": [
            {
                "action": {
                    "type": "list",
                    "actions": [
                        {
                            "type": "waitcss",
                            "target": {
                                "selector": ".shp"
                            },
                            "retries": "2",
                            "waitTime": "250"
                        },
                        {
                            "type": "click",
                            "target": {
                                "selector": ".shp"
                            },
                            "parent": {
                                "selector": ".pdynamicbutton"
                            }
                        },
                        {
                            "type": "waitcss",
                            "target": {
                                "selector": ".pdynamicbutton .advance"
                            },
                            "retries": "2",
                            "waitTime": "250"
                        },
                        {
                            "type": "ifcss",
                            "target": {
                                "selector": ".prefPanel > div[role~='group']"
                            },
                            "trueAction": {
                                "type": "list",
                                "actions": [
                                    {
                                        "type": "click",
                                        "target": {
                                            "selector": ".pdynamicbutton .advance"
                                        }
                                    },
                                    {
                                        "type": "waitcss",
                                        "target": {
                                            "selector": ".switch span.on"
                                        },
                                        "retries": "100",
                                        "waitTime": "250"
                                    }
                                ]
                            }
                        },
                        {
                            "type": "ifcss",
                            "target": {
                                "selector": ".cookiecat"
                            },
                            "trueAction": {
                                "type": "waitcss",
                                "target": {
                                    "selector": ".switch span.on"
                                },
                                "retries": "25",
                                "waitTime": "250"
                            }
                        }
                    ]
                },
                "name": "OPEN_OPTIONS"
            },
            {
                "action": {
                    "type": "list",
                    "actions": [
                        {
                            "type": "foreach",
                            "target": {
                                "selector": ".cookiecat"
                            },
                            "action": {
                                "type": "list",
                                "actions": [
                                    {
                                        "type": "ifcss",
                                        "target": {
                                            "selector": ".gwt-InlineHTML",
                                            "textFilter": [
                                                "Functional",
                                                "Funktionalitets",
                                                "Funktionelle Cookies"
                                            ]
                                        },
                                        "trueAction": {
                                            "type": "consent",
                                            "consents": [
                                                {
                                                    "matcher": {
                                                        "type": "css",
                                                        "target": {
                                                            "childFilter": {
                                                                "target": {
                                                                    "selector": "span[role='option'].active",
                                                                    "textFilter": "On"
                                                                }
                                                            },
                                                            "selector": ".switch"
                                                        }
                                                    },
                                                    "trueAction": {
                                                        "type": "click",
                                                        "target": {
                                                            "selector": ".switch span.on[role='option']"
                                                        }
                                                    },
                                                    "falseAction": {
                                                        "type": "click",
                                                        "target": {
                                                            "selector": ".switch span.off[role='option']"
                                                        }
                                                    },
                                                    "type": "A"
                                                }
                                            ]
                                        }
                                    },
                                    {
                                        "type": "ifcss",
                                        "target": {
                                            "selector": ".gwt-InlineHTML",
                                            "textFilter": [
                                                "Marketing",
                                                "Annonceringscookies",
