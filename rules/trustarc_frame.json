                                                {
                                                    "matcher": {
                                                        "type": "css",
                                                        "target": {
                                                            "childFilter": {
                                                                "target": {
                                                                    "selector": "span[role='option'].active",
                                                                    "textFilter": "On"
                                                                }
                                                            },
                                                            "selector": ".switch"
                                                        }
                                                    },
                                                    "trueAction": {
                                                        "type": "click",
                                                        "target": {
                                                            "selector": ".switch span.on[role='option']"
                                                        }
                                                    },
                                                    "falseAction": {
                                                        "type": "click",
