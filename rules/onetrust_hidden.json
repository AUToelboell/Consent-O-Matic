{
    "onetrust_hidden": {
        "detectors": [
            {
                "presentMatcher": {
                    "type": "css",
                    "target": {
                        "selector": ".optanon-toggle-display"
                    }
                },
                "showingMatcher": {
                    "type": "css",
                    "target": {
                        "selector": ".optanon-toggle-display",
                        "displayFilter": true
                    }
                }
            }
        ],
        "methods": [
            {
                "name": "HIDE_CMP"
            },
            {
                "name": "OPEN_OPTIONS"
            },
            {
                "name": "DO_CONSENT"
            },
            {
                "name": "SAVE_CONSENT"
            },
            {
                "action": {
                    "type": "click",
                    "target": {
                        "selector": ".optanon-toggle-display"
                    }
                },
                "name": "UTILITY"
            }
        ]
    }
}