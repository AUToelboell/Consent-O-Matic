{
    "$schema": "https://raw.githubusercontent.com/cavi-au/Consent-O-Matic/master/rules.schema.json",
    "tealium.com1": {
        "detectors": [
            {
                "presentMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": "#__tealiumGDPRecModal, script[id^=tealium]"
                        }
                    }
                ],
                "showingMatcher": [
                    {
                        "type": "css",
                        "target": {
                            "selector": "#__tealiumGDPRecModal .privacy_prompt, .cookie_banner_background,#__tealiumGDPRecModal .consent_prompt"
                        }
                    }
                ]
            }
