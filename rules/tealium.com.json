                                        }
                                    },
                                    "toggleAction": {
                                        "type": "click",
                                        "target": {
                                            "selector": "label[for=toggle_cat1]"
                                        },
                                        "parent": {
                                            "childFilter": {
                                                "target": {
                                                    "selector": "#toggle_cat1"
